{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": {
    "*.js": {
      "maxDuration": 60 // Set duration in seconds (up to 60 seconds)
    }
  }
  }
